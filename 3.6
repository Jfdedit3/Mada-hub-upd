-- Create a script that downloads and executes external code from a link

local link = "https://raw.githubusercontent.com/Jfdedit3/Jfdedit3/refs/heads/main/Mada%20hub%20v3.6" -- Link to the script to execute

-- Function to download and execute the script from the link
local function executeScriptFromLink(url)
local success, result = pcall(function()
local scriptSource = game:HttpGet(url) -- Retrieve the content of the script
loadstring(scriptSource)() -- Execute the downloaded script
end)

-- If an error occurs, display a message
if not success then
warn("Error executing script: " .. result)
end
end

-- Launch the execution of the script from the link
executeScriptFromLink(link) 
